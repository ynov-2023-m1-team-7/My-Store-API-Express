generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_URL")
}

model product {
  id          Int     @id @default(autoincrement())
  name        String
  description String?
  active      Boolean @default(false)
  thumbnail   String
  packshot    String @default("/uploads/")
  price       Decimal @default(0.00)
}
