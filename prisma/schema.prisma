generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Product {
  id          Int     @id @default(autoincrement())
  name        String
  description String?
  active      Boolean @default(false)
  thumbnail   String
  packshot    String @default("/uploads/")
  price       Decimal @default(0.00)
}
